  "validationCriteria": {
    "testCases": {
      "minTestCases": 10,
      "maxTestCases": 30,
      "requiredFields": [
        "testCaseId",
        "description",
        "preconditions",
        "steps",
        "expectedResults",
        "priority",
        "type"
      ],
      "fieldRequirements": {
        "testCaseId": "Format: TC_MODULE_XXX",
        "description": "50-150 characters, specific and actionable",
        "preconditions": "Array of 1-3 specific preconditions",
        "steps": "Array of 3-8 detailed, numbered steps",
        "expectedResults": "30-100 characters, specific and measurable",
        "priority": "High, Medium, or Low",
        "type": "Positive, Negative, or Edge Case"
      },
      "coverageDistribution": {
        "positive": ">= 30%",
        "negative": ">= 30%",
        "edgeCase": ">= 30%"
      }
    },
    "testPlans": {
      "requiredSections": [
        "Test Objectives",
        "Scope",
        "Test Approach/Strategy",
        "Test Types",
        "Test Environment Requirements",
        "Test Data Requirements",
        "Risk Assessment",
        "Timeline/Estimation",
        "Resource Requirements",
        "Entry/Exit Criteria"
      ],
      "minLength": 500,
      "qualityIndicators": [
        "No placeholder text",
        "Specific timelines and estimates",
        "Actionable test approach",
        "Clear scope definition"
      ]
    },
    "bugReports": {
      "requiredSections": [
        "Title/Summary",
        "Description",
        "Steps to Reproduce",
        "Expected Behavior",
        "Actual Behavior",
        "Environment",
        "Priority/Severity"
      ],
      "minLength": 200,
      "qualityIndicators": [
        "Minimum 3 detailed steps",
        "Clear expected vs actual behavior",
        "Specific environment details",
        "Actionable for developers"
      ]
    }
  }
